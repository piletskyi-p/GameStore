<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>

  <connectionStrings>
    <!--<add name="OnlineStore" connectionString="data source=(LocalDb)\MSSQLLocalDB;Initial Catalog=OnlineStore.mdf;Integrated Security=True;"
         providerName="System.Data.SqlClient"/>-->
    <add name="GameStore" connectionString="Data Source=localhost;Initial Catalog=GameStore.mdf; Integrated Security=True" providerName="System.Data.SqlClient" />
    <!--<add name="Northwind" connectionString="mongodb://ecsd00300253.epam.com:8372/{NameSurname}" xdt:Transform="Replace" xdt:Locator="Match(name)"/>-->
    <add name="MongoDB" connectionString="mongodb://localhost:27017/Northwind" />
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
  </appSettings>
  <system.web>
    <customErrors mode="On">
      <error statusCode="400" redirect="~/Content/ErrorPages/Status400.html" />
      <error statusCode="404" redirect="~/Content/ErrorPages/Status404.html" />
      <error statusCode="500" redirect="~/Content/ErrorPages/Status500.html" />
    </customErrors>

    <httpHandlers>

    </httpHandlers>

    <compilation debug="true" targetFramework="4.6.1" />
    <httpRuntime targetFramework="4.6.1" maxRequestLength="1048576" />

    <!--<httpModules>
        <add name="AuthHttpModule" type="GameStore.Web.AuthHttpModule, GameStore.Web"/>
    </httpModules>-->

    <pages pageBaseType="System.Web.Mvc.WebViewPage">
      <namespaces>
        <add namespace="GameStore.Web.App_LocalResources" />
      </namespaces>
    </pages>
  </system.web>

  <system.webServer>



    <modules>
      <add name="AuthHttpModule" type="GameStore.Web.AuthHttpModule" />
    </modules>
    <!--<validation validateIntegratedModeConfiguration="false"/>-->
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <!--<add verb="*" name ="ImageHandler" path="*"
           type="GameStore.Web.Handlers.ImageHandler"/>-->
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
      <add verb="*" name="ImageHandler1" path="*.jpg" type="GameStore.Web.Handlers.ImageHandler" />
      <add verb="*" name="ImageHandler2" path="*.jpeg" type="GameStore.Web.Handlers.ImageHandler" />
      <add verb="*" name="ImageHandler3" path="*.png" type="GameStore.Web.Handlers.ImageHandler" />
    </handlers>
  </system.webServer>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Autofac" publicKeyToken="17863af14b0044da" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-4.8.1.0" newVersion="4.8.1.0" />
